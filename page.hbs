{{!< default}}

{{#post}}
<div class="wrapper">
  <article id="page">
    <section class="page" id="page-{{id}}">
        <div class="page--header">
          <h1 class="title"><a href="{{url}}">{{{title}}}</a></h1>
        </div>
        <div class="page-content">
          {{content}}
        </div>
        {{!> aside}}
    </section>
  </article>
</div>
{{/post}}
